<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<Pane xmlns:fx="http://javafx.com/fxml/1" prefHeight="300.0" prefWidth="400.0"
      style="-fx-background-color: lightseagreen;" xmlns="http://javafx.com/javafx/8"
      fx:controller="frontend.AuthorizeController">
   <Button fx:id="sign" contentDisplay="CENTER" layoutX="292.0" layoutY="230.0" mnemonicParsing="false"
           onAction="#checkLogPass" prefHeight="19.0" prefWidth="94.0" text="Sign in"/>
   <AnchorPane layoutX="139.0" layoutY="40.0"/>
   <Label layoutX="114.0" layoutY="40.0" prefHeight="53.0" prefWidth="172.0" text="  Welcome" textAlignment="CENTER"
          wrapText="true">
      <font>
         <Font size="32.0"/>
      </font>
   </Label>
   <PasswordField fx:id="pass" layoutX="236.0" layoutY="190.0"/>
   <Label layoutX="183.0" layoutY="189.0" text="Pass">
      <font>
         <Font size="18.0"/>
      </font>
   </Label>
   <TextField fx:id="login" layoutX="236.0" layoutY="151.0"/>
   <Label layoutX="173.0" layoutY="150.0" text="Login">
      <font>
         <Font size="18.0"/>
      </font>
   </Label>
   <Label fx:id="errorMessage" layoutX="168.0" layoutY="268.0" prefHeight="27.0" prefWidth="223.0" text="EROR"
          textFill="RED">
      <font>
         <Font size="15.0"/>
      </font>
   </Label>
   <Button fx:id="reg" contentDisplay="CENTER" layoutX="14.0" layoutY="269.0" mnemonicParsing="false"
           onAction="#gotoReg" prefHeight="19.0" prefWidth="94.0" text="Registration"/>
</Pane>
